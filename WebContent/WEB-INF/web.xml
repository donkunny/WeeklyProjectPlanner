<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>WeeklyProjectPlanner</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
	<servlet>
		<servlet-name>ProjectController</servlet-name>
		<servlet-class>com.lotte.project.controller.ProjectController</servlet-class>
	</servlet>  
	<servlet-mapping>
		<servlet-name>ProjectController</servlet-name>
		<url-pattern>/project</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EmpController</servlet-name>
		<servlet-class>com.lotte.emp.controller.EmpController</servlet-class>
	</servlet>  
	<servlet-mapping>
		<servlet-name>EmpController</servlet-name>
		<url-pattern>/emp</url-pattern>
	</servlet-mapping>
</web-app>